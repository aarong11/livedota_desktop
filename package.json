{
  "name": "livedota_desktop",
  "version": "0.0.0",
  "description": "Testing",
  "main": "./desktop_client_electron/index.js",
  "scripts": {
    "bundle": "webpack --config config/webpack.web.js --progress --profile --bail",
    "bundle:electron": "webpack --config config/webpack.electron.js --progress --profile --bail",
    "cordova": "npm run bundle && cordova run android",
    "web:dev": "npm run bundle && webpack-dev-server --content-base dist/web",
    "electron": "npm run bundle:electron && npm run electron:dev",
    "electron:dev": "electron ./dist/electron/index.js --debug",
    "electron:release": "npm run bundle:electron && npm run electron:build-deps && npm run electron:pack-win",
    "electron:build-deps": "cd ./dist/electron &&  npm install --production",
    "electron:pack-mac": "electron-packager ./dist/electron --no-prune --overwrite --platform=darwin --arch=x64 --out=dist/electron-darwin",
    "electron:pack-win": "electron-packager ./dist/electron --no-prune --overwrite --platform=win32 --arch=ia32 --out=dist/electron-windows-ia32",
    "electron:pack-linux": "electron-packager ./dist/electron --no-prune --overwrite --platform=linux --arch=x64 --out=dist/electron-linux-x86"
  },
  "author": "Aaron Goulden <aaron@molecular-creations.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joaogarin/angular2-electron/issues"
  },
  "homepage": "",
  "engines": {
    "node": ">= 4.2.1 <= 6",
    "npm": ">= 3"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.64",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "concurrently": "3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cordova": "^6.5.0",
    "css-loader": "^0.26.1",
    "electron-directory": "0.0.3",
    "electron-packager": "^8.5.0",
    "electron-windows-store": "^0.8.1",
    "electron-winstaller": "^2.5.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "null-loader": "^0.1.1",
    "only-if-changed-webpack-plugin": "0.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "signcode": "^0.5.0",
    "style-loader": "^0.13.1",
    "ts-loader": "1.3.3",
    "typescript": "~2.0.10",
    "typings": "^1.0.4",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.4.0",
    "webpack-target-electron-renderer": "^0.4.0",
    "webpack-validator": "^2.3.0"
  },
  "dependencies": {
    "angular": "^1.6.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron": "^1.4.15",
    "es6-shim": "0.35.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.15.2",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "systemjs": "^0.19.38",
    "typings": "^1.5.0",
    "webpack": "2.2.0",
